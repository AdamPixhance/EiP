import java.util.Arrays;
import java.util.Random;

public class DemoVektor {
	final static int DIMINSIONS = 5;
	
	private int[] komponenten = new int[DIMINSIONS];

	public static void main(String[] args) {
		DemoVektor vek1 = new DemoVektor();
		DemoVektor vek2 = new DemoVektor();
		
		initVektor(vek1);
		initVektor(vek2);
		
		System.out.println("Inhalt Vektro 1: " + Arrays.toString(vek1));
		System.out.println("Inhalt Vektro 2: " + Arrays.toString(vek2));
		
		System.out.print("Skalarprodukt: ");
		System.out.println(scalarProduct(vek1, vek2));
	}
	
	public static int scalarProduct(int[] v1, int[] v2) {
		int sum = 0;
		for (int i = 0; i< v1.length; i++) {
			sum += v1[i]*v2[i];
		}
		
		return sum;
	}
	
	
	public static void initVektor(int[] v) {
		Random rnd = new Random();
		for (int i = 0; i < v.length; ++i) {
			v[i] = rnd.nextInt(100, 200);
		}
	}

}
